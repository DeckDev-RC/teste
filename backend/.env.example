# =================================
# Backend - Variáveis de Ambiente
# =================================

# Chaves de API (podem ser múltiplas separadas por vírgula)
GEMINI_API_KEYS=sua_chave_aqui
OPENAI_API_KEYS=sua_chave_aqui
OPENROUTE_API_KEYS=chave1,chave2

# Supabase (Self-hosted)
SUPABASE_URL=http://your-ip:8000
SUPABASE_SERVICE_KEY=your-service-key
SUPABASE_ANON_KEY=your-anon-key

# Evolution API
EVOLUTION_API_URL=https://evolution.seudominio.com.br
EVOLUTION_API_KEY=sua_chave_evolution

# URL pública do backend (para webhooks)
PUBLIC_BACKEND_URL=https://leitordedoc.seudominio.com.br

# Google Drive (Integração Fase 2)
GOOGLE_DRIVE_CLIENT_EMAIL=seu-email-service-account@projeto.iam.gserviceaccount.com
GOOGLE_DRIVE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
GOOGLE_DRIVE_FOLDER_ID=id-da-pasta-no-drive

# Configuração do servidor
PORT=3001
NODE_ENV=production
CORS_ORIGIN=https://leitordedoc.seudominio.com.br

